  <%= csrf_meta_tags %>


</head>
	<body>
		
		<!-- Outer Starts -->
		<div class="outer">
			
			<!-- Top bar starts -->
			<div class="top-bar">
				<div class="container">
				
					<!-- Contact starts -->
					<div class="tb-contact pull-left">
						<!-- Email -->
						<i class="fa fa-envelope color"></i> &nbsp; <a href="mailto:contact@domain.com">jmixa6@gmail.com</a>
						&nbsp;&nbsp;  
						<!-- Phone -->
						<i class="fa fa-phone color"></i> &nbsp; +1 (815)-245-8834
					</div>
					<!-- Contact ends -->
					
					<!-- Search section for responsive design -->
					<div class="tb-search pull-left">
						<a href="#" class="b-dropdown"><i class="fa fa-search square-2 rounded-1 bg-color white"></i></a>
						<div class="b-dropdown-block">
							<form role="form">
								<!-- Input Group -->
								<div class="input-group">
									<input type="text" class="form-control" placeholder="Type Something">
									<span class="input-group-btn">
										<button class="btn btn-color" type="button">Search</button>
									</span>
								</div>
							</form>
						</div>
					</div>
					<!-- Search section ends -->
					

					<!-- Social media ends -->
					
					<div class="clearfix"></div>
				</div>
			</div>
			
			<!-- Top bar ends -->
			
			<!-- Header two Starts -->
			<div class="header-2">
			
				<!-- Container -->
				<div class="container">
					<div class="row">
						<div class="col-md-2">
							<!-- Logo section -->
							<div class="logo">
								<h1><a href="/"><i class="fa fa-laptop"></i>Fragit</a></h1>
							</div>
						</div>
						<div class="col-md-3">
						</div>


						
						<div class="col-md-4">
							
							<!-- Search section -->
							<div class="head-search pull-right">
								<a href="#" class="b-dropdown"><i class="fa fa-search square-2 rounded-1 bg-color white"></i></a>
								<div class="b-dropdown-block">
									<form role="form">
										<!-- Input Group -->
										<div class="input-group">
											<input type="text" class="form-control" placeholder="Type Something">
											<span class="input-group-btn">
												<button class="btn btn-color" type="button">Search</button>
											</span>
										</div>
									</form>
								</div>
							</div>
						</div>
							
						<div class="col-md-3">
					
							<!-- Navigation starts.  -->
							<div class="navy">			
								<ul>
									<!-- Main menu -->
									<li><a href="/items">Explore</a>
										<!-- Submenu -->
										<ul>
											<li><a href="/items" class="black">Available Corals</a></li>
											<li><a href="/items/new" class="black">Post New Corals</a></li>

											<li><a href="/requests" class="black">Open Requests</a></li>
											<li><a href="/requests/new" class="black">Post New Request</a></li>

											<li><a href="/holders" class="black">Lineages</a></li>
											<li><a href="/holders/new" class="black">Request New Lineage</a></li>
								
											<li><a href="/images">Images</a></li>
										</ul>
									</li>
									<li><a href="/profiles">Profiles</a>
										<ul>
											<% if current_user %>
											<li><%= link_to "Sign Out", "/users/sign_out", method: :delete %></li>
											
											<% else %>
											<li><%= link_to "Sign Up", "/users/sign_up" %></li>
											<li><%= link_to "Sign In", "/users/sign_in" %></li>
											<% end %>
											
											<!--Needs more work -->
										
											<li><a href="/profiles/<%= current_user.id %>">Your Profile</a>
									
											<li><a href="/profiles/admin">Admin</a></li>
											
										</ul>
									</li>	
								</ul>
							</div>							
							<!-- Navigation ends -->
							
							<!-- Search section ends -->
							<div class="clearfix"></div>
							
						</div>
						
					</div>
				</div>
			</div>
	
